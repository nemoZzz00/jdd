<div class="footer">
	<div  class="w">
		<div class="gotop" style="display:none;"><a href="#">gotop</a></div>
		<h3>
			<% if (typeof copyright != "undefined"){ %>
				<%- copyright %>
			<% } %>
			<i><%= name %>@<%= date %></i>
			<% if (typeof stat != "undefined"){ %>
				 <i><%- stat %></i>
			<% } %>
		</h3>
	</div>
